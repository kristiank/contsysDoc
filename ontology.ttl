                                                                 ] ,
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty :isPlannedIn ;
                                                                   owl:someValuesFrom <https://contsys.org/concept/care_plan>
                                                                 ] ,
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty :isPerformedFor ;
                                                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
